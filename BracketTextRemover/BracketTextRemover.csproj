<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <title>BracketTextRemover</title>
    <RepositoryUrl>https://github.com/jacob9423/Bracket-Text-Remover</RepositoryUrl>
    <PackageId>BracketTextRemover</PackageId>
    <Version>1.0.0</Version>
    <Authors>Jacob_Barfield</Authors>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    
